<% provide(:title, "Edit subforum") %>
<h1>Update the Subforum</h1>

<div class="form-wrapper">
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@subforum) do |f| %>
      <%= render 'shared/error_messages', object: f.object %>

      <%= f.label :subforumname, 'New Subforum Name' %>
      <%= f.text_field :subforumname, class: 'form-control' %>

      <%= f.submit "Save changes", class: "btn btn-primary" %>
    <% end %>

  </div>
</div>
</div>
