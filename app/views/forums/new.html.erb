<% provide(:title, 'Create Forum') %>
<h1>Create Forum</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@forum) do |f| %>
	  
      <%= f.label :forumname, 'Forum Name' %>
      <%= f.text_field :forumname, class: 'form-control' %>

      <%= f.submit "Submit", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>